local DiscordLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/discord"))()

-- Yeni bir DiscordLib penceresi oluştur
local win = DiscordLib:Window("FiP (Bug Fixes And BACK ON DEAD :skull:)")

-- Bildirim fonksiyonu
local function notify(title, text)
    DiscordLib:Notification(title, text, "Okay")
end

-- Fonksiyon: Butonları ve script URL'lerini ekleyen kanal oluşturma
local function createButtonChannel(channel, buttonNames, scriptFunctions)
    for i, buttonName in ipairs(buttonNames) do
        channel:Button(buttonName, function()
            local scriptFunction = scriptFunctions[i] or function() end -- Script fonksiyonunu seç
            -- Bildirim gönder: Butona basıldığında
            notify("Script Executed", "Running script: " .. buttonName)
            -- İlgili scripti çalıştır
            local success, result = pcall(scriptFunction)
            if not success then
                DiscordLib:Notification("Error", "Failed to execute script: " .. tostring(result), "Okay")
            end
        end)
    end
end

-- Fonksiyon: Sunucu oluşturma
local function createServer(name, iconId)
    local server = win:Server(name, "http://www.roblox.com/asset/?id=" .. iconId)
    return server
end

