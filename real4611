   -- DiscordLib'i yükle
local DiscordLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/discord"))()

-- Yeni bir DiscordLib penceresi oluştur
local win = DiscordLib:Window("The Biggest Custom (Movesets Hub Added New Customs❗)")

-- Fonksiyon: Butonları ve script URL'lerini ekleyen tab oluşturma
local function createButtonPage(tabName, buttonNames, scriptFunctions)
    local tab = win:Server(tabName, "") -- Tab oluştur
    local btnChannel = tab:Channel("Buttons")

    for i, buttonName in ipairs(buttonNames) do
        btnChannel:Button(buttonName, function()
            local scriptFunction = scriptFunctions[i] or function() end -- Script fonksiyonunu seç
            -- İlgili scripti çalıştır
            local success, result = pcall(scriptFunction)
            if not success then
                print("Failed to execute script:", result)
            end
        end)
    end
end

