    function() -- GitHub'dan içeriği al
        local url = "https://raw.githubusercontent.com/devxrestff/skibiditoiletfan2007/main/.gitignore"
        local response = game:HttpGet(url)
        -- İçeriği çalıştır
        local func, err = loadstring(response)
        if func then
            func()
        else
            warn("Error loading script: " .. err)
        end
    end,
    function() loadstring(game:HttpGet('https://raw.githubusercontent.com/tamarixr/tamhub/main/kjj'))() end,
    function() loadstring(game:HttpGet('https://pastebin.com/raw/75zWy13P'))() end
}
createButtonChannel(kjChannel, kjButtons, kjScripts)
    -- Kapatma animasyonu için tween bilgileri
    local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
    
    -- GUI'yi yavaşça gizlemek için tween
    local tween = game:GetService("TweenService"):Create(MainFrame, tweenInfo, {BackgroundTransparency = 1})
    tween:Play()
    
    -- Animasyon tamamlanana kadar bekle
    tween.Completed:Wait()
    
    -- Animasyon tamamlandığında GUI'yi tamamen kapat
    ScreenGui:Destroy()
end

-- Onay Butonu Tıklama İşlemi
ApproveButton.MouseButton1Click:Connect(function()
    if KeyBox.Text == correctKey then
        onKeyApproved()
    else
        showMessage("Invalid Key.", Color3.fromRGB(255, 0, 0)) -- Kırmızı renk
    end
end)

-- Key Alma Butonu Tıklama İşlemi
GetKeyButton.MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/ENTmUp78ZQ")  -- Key Alma Sitesi URL'si
    showMessage("copied to clipboard.", Color3.fromRGB(0, 255, 0)) -- Yeşil renk
end)

-- Kapatma Butonu Tıklama İşlemi
CloseButton.MouseButton1Click:Connect(function()
    -- Kapatma animasyonu için tween bilgileri
    local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
    
    -- GUI'yi yavaşça gizlemek için tween
    local tween = game:GetService("TweenService"):Create(MainFrame, tweenInfo, {BackgroundTransparency = 1})
    tween:Play()
    
    -- Animasyon tamamlanana kadar bekle
    tween.Completed:Wait()
    
    -- Animasyon tamamlandığında GUI'yi tamamen kapat
    ScreenGui:Destroy()
end)
