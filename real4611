-- Diğer tabları oluştur
local function createEmptyTab(tabName)
    local tab = win:Server(tabName, "")
    local btnChannel = tab:Channel("Buttons")
    btnChannel:Button("NotDone", function()
        print(tabName .. " - This tab is not done yet.")
    end)
end

createEmptyTab("Cyborg Customs")
createEmptyTab("Metal Bat Customs")
createEmptyTab("Sonic Customs")
createEmptyTab("Atomic Customs")
createEmptyTab("Tatsumaki Customs")

-- Fix (Works✅) isminde yeni bir sunucu oluştur
local fixServ = win:Server("Fix (Works✅)", "✅")

-- Fix (Works✅) sunucusu içinde Garou Fix ve Saitama Fix kanallarını oluştur
local function createFixChannel(channelName, buttonNames, scriptFunctions)
    local channel = fixServ:Channel(channelName) -- Kanal oluştur
    for i, buttonName in ipairs(buttonNames) do
        channel:Button(buttonName, function()
            local scriptFunction = scriptFunctions[i] or function() end -- Script fonksiyonunu seç
            -- İlgili scripti çalıştır
            local success, result = pcall(scriptFunction)
            if not success then
                print("Failed to execute script:", result)
            end
        end)
    end
end

-- Garou Fix ve Saitama Fix için butonları ve script fonksiyonlarını oluştur
